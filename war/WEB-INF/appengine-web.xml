<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>mishima</application>
  <version>1.0-SNAPSHOT</version>

  <static-files>
    <include path="/**.*"/>
    <include path="/css/**.css"/>
    <include path="/css/**.png"/>
    <include path="/js/**.js"/>
    <include path="/img/**.png"/>
    <include path="/img/**.gif"/>
    <include path="/img/**.jpg"/>
    <include path="/favicon.ico"/>
  </static-files>

  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/classes/logging.properties"/>
    <!-- Session情報(参照可能プロジェクト、システム上の権限)のリフレッシュ間隔(分単位) -->
    <property name="jp.co.nemuzuka.session.refresh.min" value="60"/>
    <!-- 優先度の初期値 -->
    <property name="jp.co.nemuzuka.default.priority" value="高&#xA;中&#xA;低" />
    <!-- ステータスの初期値。ステータス,完了とみなすステータスのフォーマットに従って下さい -->
    <property name="jp.co.nemuzuka.default.status" value="新規&#xA;割り当て済み&#xA;対応中&#xA;差し戻し&#xA;完了&#xA;キャンセル,完了&#xA;キャンセル" />
    <!-- 種別の初期値 -->
    <property name="jp.co.nemuzuka.default.kind" value="機能&#xA;バグ&#xA;課題&#xA;要望&#xA;問い合わせ&#xA;メモ&#xA;" />
  </system-properties>
  
  <sessions-enabled>true</sessions-enabled>
</appengine-web-app>
